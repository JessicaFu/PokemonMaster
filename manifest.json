{
  "manifest_version": 2,
  "name":"Pokemon Master",
  "version": "1.1",
  "description": "Be a pokemon master",

  "browser_action":{
    "default_icon": "images/pokemon/pikachu.png",
    "default_popup": "index.html"
   },
  "background": {
    "scripts": ["js/jquery-2.1.3.min.js", "background.js"]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions":[
  	"tabs"
  ]

}
